public class Seq_Search{
    
    public int Seq_Search(int first,int last,int[] arr,int target){
        for(int i=first;i<last;i++){
            if(arr[i]==target){
                return i;
            }
        return -1;
    }
    
    public static void main(String[] args){
        int[] arr = {25,13,56,1,68};
        Seq_Search ss = new Seq_Search();
        System.out.println("目标所在位置："+ss.Seq_Search(0,arr.length,arr,13);
    }
}
